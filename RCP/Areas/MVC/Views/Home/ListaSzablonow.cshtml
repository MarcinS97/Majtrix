@model IEnumerable<HRRcp.Areas.MVC.Models.obdSzablon>

@{
    ViewBag.Title = "ListaSzablonow";
    Layout = "~/Areas/MVC/Views/Shared/_Layout.cshtml";
}

<h2>ListaSzablonow</h2>

<p>
    @Html.ActionLink("Dodaj nowy", "Szablon")
</p>
<table >
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdObieguDict)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nazwa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Opis)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdObieguDict)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nazwa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Opis)
            </td>


            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id }) |
                @Html.ActionLink("Wypelnij", "Wypelnijdokument", new { id = item.Id })
            </td>
        </tr>
    }

</table>
