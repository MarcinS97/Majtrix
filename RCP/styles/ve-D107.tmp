/*----- RAPORTY - KARTA ROCZNA -----*/
table.tbKartaRoczna th.header { height: 130px; vertical-align: top; padding: 0px;}
table.tbKartaRoczna th.suma { max-width: 28px; height: 150px; text-align: left; vertical-align: bottom; }
table.tbKartaRoczna th.suma span { xborder: solid 1px red; }
table.tbKartaRoczna th.suma span.rotate90L { display: block; font-weight: normal; font-size: 11px; white-space: nowrap; margin-bottom: 10px; }                                             
table.tbKartaRoczna th.month,
table.tbKartaRoczna td.month { text-align: center; white-space: nowrap;}

table.tbKartaRoczna tr.it td.suma { vertical-align: middle; font-size: 12px; }
table.tbKartaRoczna td.suma span.lb0 { display: inline; }
table.tbKartaRoczna td.suma1 { border-left-width: 1px; } 
table.tbKartaRoczna tr.it td.day div.day span.overlaptext { margin-top: 11px; font-size: 9px; color: Black;}

table.tbKartaRoczna tr.it td.holiday { background: none;}
table.tbKartaRoczna tr.it td.holiday div { border-top: solid 3px #FFCCD3 !important;}  /*#FF3755*/
table.tbKartaRoczna tr.it td.holiday span.toptext,
table.tbKartaRoczna tr.it td.holiday span.bottomtext { position: relative; top: -3px; }


