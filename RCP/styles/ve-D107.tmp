/*----- Form Captions -----*/
table.caption   /* dla caption w wielu liniach */
{
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 14px;
    font-weight: bold;
    border-collapse: collapse;
    white-space: nowrap;
    padding: 0px;
    margin: 0px;
}
table.caption td { padding: 16px 0px 0px 0px; vertical-align: top;}
table.caption td.caption_text { padding: 16px 0px 16px 0px; }
table.caption img { padding: 2px 16px 0px 24px; }

span.caption_icon { width: 32px; height: 48px; margin: 0px 16px 0px 0px; display:inline-block; float: left; xborder: solid 1px red; }

span.caption, span.caption1, span.caption2, span.caption3, span.caption4    /* dla caption w jednej linii */
{
    padding-left:24px;
    height: 32px;
    line-height: 32px;
    white-space: nowrap;
    vertical-align: middle;
    
    font-family:Verdana, Arial, Helvetica, sans-serif;
    font-size:14px;
    font-weight:bold;
    display: block;
    
    xbackground-color: #EEEEEE;
    xclear: right;
}

span.caption img, span.caption1 img, span.caption2 img, span.caption3 img, span.caption4 img
{
    padding-right:8px;
    float: left;
}

span.caption1   /* odstęp od góry */
{
    padding-top: 8px;
}

span.caption2   /* odstęp od dołu */
{
    padding-bottom: 8px;
}

span.caption3   /* odstęp od góry i od dołu */
{
    padding-top: 8px;
    padding-bottom: 8px;
    
    xborder: solid 1px red;
    xbackground-color: Gray;
}

span.caption4   /* odstęp od góry i od dołu */
{
    padding-top: 8px;
    padding-bottom: 16px;
}

/*----- FORM CAPTIONS #2 -----*/
div.title3 { padding:0px; margin: 0px;}
div.title3 img.ico { display: block; width: 32px; margin: 3px 16px 0px 8px; float: left;}
div.title3 span.captions { display: inline-block; }
div.title3 .captions span { vertical-align: baseline; white-space: nowrap; line-height: normal; height: auto; padding: 0px; }
div.title3 .captions span.title { display: block; font-size: 14px; font-weight: bold; color: #3F3F3F; }
div.title3 .captions span.subtitle { display: block; line-height: 1.5em; }
div.title3 .captions span.subtitle span.caption { display: inline-block; font-size: 9px; font-weight: bold; color: Gray; text-transform: uppercase; }
div.title3 .captions span.subtitle span.value { display: inline-block; xfont-weight: bold;}

/*----- Tab Control -----*/
/*----- TABS - wspólne -----*/
.tabsStrip { margin-left: 10px; }

/*----- TABS - IE -----*/
table.tabsStrip { border-collapse: collapse; empty-cells: hide; }     /* dla ie78*/
table.tabsStrip td { width: 0px !important; } /* tak wycinamy odstęp 3px między tabami, działa też na IE6 */
table.tabItem {}
table.tabItem td {} /* tabela */

/*----- TABS - Chrome -----*/
div.tabsStrip { display: inline-block; }
span.tabItem { float: left; }           /* zeby byly obok siebie */
span.tabItem[disabled] .tabRight { color: Silver; cursor: auto;}

/*-----*/
div.tabCaption
{
    display: block;
    padding: 0px 0px 0px 0px;
    background:url(../images/tab2_m.jpg) repeat-x top; 
    height:23px;
}

div.tabLeft
{
    display: block;
    background:url(../images/tab2_l.jpg) no-repeat left top; 
    height: 23px;
}

div.tabRight
{
    display: block;
    background:url(../images/tab2_r.jpg) no-repeat right top; 
    
    padding: 2px 10px 0px 10px; 
    height: 21px;       /* padding 2px */
    line-height: 20px;  /* tak wyglada lepiej */

    font-size: 10px; 
    color: #333; 
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 12px; 
    white-space: nowrap;
    
    position: relative;
    right: -1px;
}

.tabSelected .tabCaption {background:url(../images/tab2_msely.jpg) repeat-x top; position: relative; top: 1px;}
.tabSelected .tabLeft {background:url(../images/tab2_lsel.jpg) no-repeat left top; }
.tabSelected .tabRight {background:url(../images/tab2_rsel.jpg) no-repeat right top; }

.tabHover .tabCaption {background:url(../images/tab2_mselb.jpg) repeat-x top; position: relative; top: 1px; cursor: hand;} /* na ie7 nie zmieniał kursora */
.tabHover .tabLeft {background:url(../images/tab2_lsel.jpg) no-repeat left top; }
.tabHover .tabRight {background:url(../images/tab2_rsel.jpg) no-repeat right top; }

span.tabItem:hover .tabCaption {background:url(../images/tab2_mselb.jpg) repeat-x top; position: relative; top: 1px;} /*na chrome nie działa hover z menu*/
span.tabItem:hover .tabLeft {background:url(../images/tab2_lsel.jpg) no-repeat left top; }
span.tabItem:hover .tabRight {background:url(../images/tab2_rsel.jpg) no-repeat right top; } 

span.tabItem[disabled]:hover .tabCaption {background:url(../images/tab2_m.jpg) repeat-x top; top: 0px;} /*na chrome nie działa disabled*/
span.tabItem[disabled]:hover .tabLeft {background:url(../images/tab2_l.jpg) no-repeat left top; }
span.tabItem[disabled]:hover .tabRight {background:url(../images/tab2_r.jpg) no-repeat right top; } 


